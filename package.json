{
  "name": "phapp-schedule-link",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext ts,tsx src",
    "lint:fix": "pnpm lint --fix ",
    "format:check": "prettier --config prettier.config.js --check \"**/*.{ts,tsx,html}\"",
    "format:fix": "prettier --config prettier.config.js --write \"**/*.{ts,tsx,html}\"",
    "codegen": "npx graphql-codegen --watch --config graphql.config.ts",
    "prepare": "husky || true",
    "ssl:local": "zx ./sapia.mjs setup:ssl",
    "test": "env-cmd -f .env.test jest --config ./jest/unit.config.ts --forceExit --runInBand --detectOpenHandles --passWithNoTests --no-caches",
    "test:incremental": "env-cmd -f .env.test jest --config ./jest/unit-incremental.config.ts --forceExit --runInBand --detectOpenHandles --passWithNoTests --no-caches",
    "test:report:zip": "zip -r coverage.zip coverage",
    "tdd": "pnpm test -- --watch"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.20.0",
    "@ant-design/icons": "5.3.7",
    "@apollo/client": "3.10.4",
    "@ph/ui": "0.0.67",
    "@radix-ui/react-accordion": "1.1.2",
    "@rollbar/react": "0.12.0-beta",
    "ahooks": "3.8.0",
    "antd": "5.18.0",
    "clsx": "2.1.1",
    "dayjs": "1.11.11",
    "graphql": "16.8.1",
    "graphql-config": "5.0.3",
    "js-cookie": "3.0.5",
    "libphonenumber-js": "1.11.2",
    "lodash-es": "4.17.21",
    "react": "^18.2.0",
    "react-aria": "3.33.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.23.1",
    "react-select": "5.8.0",
    "react-stately": "3.31.1",
    "rollbar": "2.26.4",
    "uuid": "9.0.1",
    "validator": "13.12.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.2.6",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-typed-document-node/core": "3.2.0",
    "@react-types/combobox": "3.11.1",
    "@react-types/shared": "3.23.1",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.12",
    "@types/js-cookie": "3.0.6",
    "@types/node": "20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "9.0.8",
    "@types/validator": "13.11.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "10.4.19",
    "env-cmd": "10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-unused-imports": "4.0.0",
    "husky": "9.0.11",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-transformer-svg": "2.0.2",
    "jsdom": "24.1.0",
    "lint-staged": "15.2.5",
    "postcss": "8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "3.4.3",
    "ts-jest": "29.1.3",
    "ts-node": "10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "4.3.2",
    "zx": "8.1.1"
  },
  "volta": {
    "node": "20.11.0",
    "npm": "10.2.3",
    "pnpm": "9.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "pnpm format:fix"
    ]
  }
